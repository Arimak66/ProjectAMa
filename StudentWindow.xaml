<Window x:Class="ProjectAMa.StudentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectAMa"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="StudentWindow" Height="450" Width="800" Background="LightBlue">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="69*"/>
            <RowDefinition Height="365*"/>
        </Grid.RowDefinitions>
        <Button Content="Button" HorizontalAlignment="Left" Height="46" Margin="50,45,0,0" Grid.Row="1" VerticalAlignment="Top" Width="132" Click="Button_Click"/>
        <TextBox Name="welcomeText" HorizontalAlignment="Left" Height="31" Margin="36,18,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="419"/>

    </Grid>
</Window>
