<Window x:Class="ProjectAMa.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectAMa"
        mc:Ignorable="d"
                WindowStartupLocation="CenterScreen"

        Title="MainWindow" Height="450" Width="800">


    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="57*"/>
            <RowDefinition Height="53*"/>
            <RowDefinition Height="75*"/>
            <RowDefinition Height="57*"/>
            <RowDefinition Height="75*"/>
            <RowDefinition Height="47*"/>
            <RowDefinition Height="70*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="153*"/>
            <ColumnDefinition Width="124*"/>
            <ColumnDefinition Width="133*"/>
            <ColumnDefinition Width="160*"/>
            <ColumnDefinition Width="230*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="LoginButton" Content="Login" HorizontalAlignment="Center" Height="43" Margin="0,14,0,0" VerticalAlignment="Top" Width="153" Click="LoginButton_Click"/>

    </Grid>
</Window>
